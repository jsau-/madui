{
  "name": "madui",
  "version": "0.0.1",
  "description": "madUI library",
  "main": "./build/index.js",
  "scripts": {
    "check:types": "tsc --noEmit",
    "doc": "del-cli \"./docs/**/*\" && typedoc",
    "prebuild": "npm run doc && del-cli \"./build/**/*\"",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --ignore \"src/**/*.unit.test.ts\",\"src/**/*.unit.test.tsx\",\"src/**/*.stories.tsx\"",
    "build": "npm run prebuild && npm run build:types && npm run build:js",
    "lint": "eslint --ext .ts --ext .tsx src --color",
    "lint:fix": "eslint --ext .ts --ext .tsx src --color --fix",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsau-/madui.git"
  },
  "keywords": [
    "react",
    "libary"
  ],
  "author": "Jon Saunders",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/jsau-/madui/issues"
  },
  "homepage": "https://github.com/jsau-/madui#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.7.7",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-essentials": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.0.21",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.5.0",
    "@types/jest": "24.0.25",
    "@types/lodash": "4.14.161",
    "@types/node": "13.9.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-typescript-to-proptypes": "1.4.1",
    "cross-env": "5.2.0",
    "del-cli": "2.0.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "jest": "24.9.0",
    "jest-html-reporter": "2.8.0",
    "lodash": "4.17.20",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-docgen-typescript-loader": "3.7.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-dom": "16.9.0",
    "react-jss": "10.4.0",
    "react-test-renderer": "16.9.0",
    "ts-jest": "24.2.0",
    "typedoc": "0.15.6",
    "typescript": "3.7.4"
  },
  "peerDependencies": {
    "lodash": "^4.7.20",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-jss": "^10.4.0"
  },
  "dependencies": {}
}
